<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.github.darryring.mybatis.mapper.UserMapper">
    <select id="selectUser" resultType="io.github.darryring.mybatis.entity.UserDO">
        select *
        from user
        where id = #{id}
    </select>
</mapper>